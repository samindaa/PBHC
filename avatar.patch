+@torch.jit.script
+def quat_unit(a):
+    return normalize(a)
+
+@torch.jit.script
+def quat_apply(a: Tensor, b: Tensor, w_last: bool) -> Tensor:
+    shape = b.shape
+    a = a.reshape(-1, 4)
+    b = b.reshape(-1, 3)
+    if w_last:
+        xyz = a[:, :3]
+        w = a[:, 3:]
+    else:
+        xyz = a[:, 1:]
+        w = a[:, :1]
+    t = xyz.cross(b, dim=-1) * 2
+    return (b + w * t + xyz.cross(t, dim=-1)).view(shape)
+
+@torch.jit.script
+def quat_apply_yaw(quat: Tensor, vec: Tensor, w_last: bool) -> Tensor:
+    quat_yaw = quat.clone().view(-1, 4)
+    quat_yaw[:, :2] = 0.0
+    quat_yaw = normalize(quat_yaw)
+    return quat_apply(quat_yaw, vec, w_last)
+
+
+@torch.jit.script
+def wrap_to_pi(angles):
+    angles %= 2*np.pi
+    angles -= 2*np.pi * (angles > np.pi)
+    return angles
+
+
+@torch.jit.script
+def quat_conjugate(a: Tensor, w_last: bool) -> Tensor:
+    shape = a.shape
+    a = a.reshape(-1, 4)
+    if w_last:
+        return torch.cat((-a[:, :3], a[:, -1:]), dim=-1).view(shape)
+    else:
+        return torch.cat((a[:, 0:1], -a[:, 1:]), dim=-1).view(shape)
+
+
+@torch.jit.script
+def quat_apply(a: Tensor, b: Tensor, w_last: bool) -> Tensor:
+    shape = b.shape
+    a = a.reshape(-1, 4)
+    b = b.reshape(-1, 3)
+    if w_last:
+        xyz = a[:, :3]
+        w = a[:, 3:]
+    else:
+        xyz = a[:, 1:]
+        w = a[:, :1]
+    t = xyz.cross(b, dim=-1) * 2
+    return (b + w * t + xyz.cross(t, dim=-1)).view(shape)
+
+
+@torch.jit.script
+def quat_rotate(q: Tensor, v: Tensor, w_last: bool) -> Tensor:
+    shape = q.shape
+    if w_last:
+        q_w = q[:, -1]
+        q_vec = q[:, :3]
+    else:
+        q_w = q[:, 0]
+        q_vec = q[:, 1:]
+    a = v * (2.0 * q_w**2 - 1.0).unsqueeze(-1)
+    b = torch.cross(q_vec, v, dim=-1) * q_w.unsqueeze(-1) * 2.0
+    c = (
+        q_vec
+        * torch.bmm(q_vec.view(shape[0], 1, 3), v.view(shape[0], 3, 1)).squeeze(-1)
+        * 2.0
+    )
+    return a + b + c
+
